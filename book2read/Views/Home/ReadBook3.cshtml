@using book2read.Models;
@model List<NovelPageViewModel>

    <h2>@ViewData["bookTitle"]</h2>
<div>
    @if(Model != null && Model.Any())
    {
        <ul>
            @foreach (var item in Model)
            {
                <li>
                    <a href="@item.Url">@item.Name</a>
                </li>
            }
        </ul>
    }
    else
    {
        <p>无数据返回</p>
    }
</div>
